"<u>Tastenfeld</u>\n"
"  Alt + 2,4,6,8: Hälfte\n"
"  Alt + 1,3,7,9: Viertel\n"
"  Alt + 5: Mitte\n"
"  Alt + 0: Umschalten maximiert"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"<u>Zifferntasten</u> \n"
"  Strg + 1-4: Hälfte\n"
"  Strg + 5-8: Viertel\n"
"  Strg + 9: Drittel \n"
"  Strg + 0: Umschalten maximiert"

