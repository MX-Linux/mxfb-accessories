#: ../mxfb-tiles:21
msgid ""
"<u>Arrow keys</u>\n"
"  Alt + Arrow: half \n"
"  Alt + LogoKey + Arrow: quarter\\n"
msgstr ""
"Tasti freccia\n"
"Alt + Freccia: metà\n"
"Alt + Tasto logo + Freccia: un quarto\n"

#: ../mxfb-tiles:24
msgid ""
"<u>Keypad</u>\n"
"  Alt + 2,4,6,8: half\n"
"  Alt + 1,3,7,9: quarter\n"
"  Alt + 5: center\n"
"  Alt + 0: toggle maximize"
msgstr ""
"Tastierino numerico\n"
"Alt + 2,4,6,8: metà\n"
"Alt + 1,3,7,9: un quarto\n"
"Alt + 5: centra\n"
"Alt + 0: attiva l'ingrandimento"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"Tasti numerici\n"
"Control + 1-4: metà\n"
"Control + 5-8: un quarto\n"
"Control + 9: un terzo\n"
"Control + 0: attiva l'ingrandimento"

