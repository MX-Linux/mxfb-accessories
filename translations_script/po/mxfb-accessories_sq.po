"  Alt + 1,3,7,9: çerek\n"
"  Alt + 5: në qendër\n"
"  Alt + 0: maksimizoje/çmaksimizoje"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"<u>Taste numra</u> \n"
"  Ctrl + 1-4: gjysmë\n"
"  Ctrl + 5-8: çerek\n"
"  Ctrl + 9: një e treta \n"
