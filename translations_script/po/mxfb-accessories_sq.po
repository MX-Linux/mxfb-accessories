#: ../mxfb-tiles:21
msgid ""
"<u>Arrow keys</u>\n"
"  Alt + Arrow: half \n"
"  Alt + LogoKey + Arrow: quarter\\n"
msgstr ""
"<u>Tastet shigjetë</u>\n"
"  Alt + Shigjetë: gjysmë \n"
"  Alt + TastStemë + Shigjetë: çerek\\n"

#: ../mxfb-tiles:24
msgid ""
"<u>Keypad</u>\n"
"  Alt + 2,4,6,8: half\n"
"  Alt + 1,3,7,9: quarter\n"
"  Alt + 5: center\n"
"  Alt + 0: toggle maximize"
msgstr ""
"<u>Pjesa numerike</u>\n"
"  Alt + 2,4,6,8: gjysmë\n"
"  Alt + 1,3,7,9: çerek\n"
"  Alt + 5: në qendër\n"
"  Alt + 0: maksimizoje/çmaksimizoje"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"<u>Taste numra</u> \n"
"  Ctrl + 1-4: gjysmë\n"
"  Ctrl + 5-8: çerek\n"
"  Ctrl + 9: një e treta \n"
"  Ctrl + 0: maksimizoje/çmaksimizoje"

