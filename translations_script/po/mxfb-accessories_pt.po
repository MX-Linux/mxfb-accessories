"<u>Teclado numérico</u>\n"
"Alt + 2,4,6,8: metade\n"
"Alt + 1,3,7,9: um quarto\n"
"Alt + 5: centrar\n"
"Alt + 0: alternar maximizar"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"<u>Teclas numéricas</u> \n"
"Control + 1-4: metade\n"
"Control + 5-8: um quarto\n"
"Control + 9: um terço \n"
"Control + 0: alterna maximizar"

