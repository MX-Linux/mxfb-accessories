#: ../mxfb-tiles:24
msgid ""
"<u>Keypad</u>\n"
"  Alt + 2,4,6,8: half\n"
"  Alt + 1,3,7,9: quarter\n"
"  Alt + 5: center\n"
"  Alt + 0: toggle maximize"
msgstr ""
"<u>कुंजीपैड</u>\n"
"  Alt + 2,4,6,8: आधा\n"
"  Alt + 1,3,7,9: एक चौथाई\n"
"  Alt + 5: केंद्रित\n"
"  Alt + 0: पूर्णतया चालू/बंद"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"<u>संख्या कुंजी</u> \n"
"  Ctrl + 1-4: आधा\n"
"  Ctrl + 5-8: एक चौथाई\n"
"  Ctrl + 9: एक तिहाई \n"
"  Ctrl + 0: पूर्णतया चालू/बंद"

