"<u>Arrow keys</u>\n"
"  Alt + Arrow: half \n"
"  Alt + LogoKey + Arrow: quarter\\n"
msgstr ""
"<u>Ok tuşları</u>\n"
"Alt +Ok: yarım\n"
"Alt + Logo tuşu + Ok: çeyrek\\n"

#: ../mxfb-tiles:24
msgid ""
"<u>Keypad</u>\n"
"  Alt + 2,4,6,8: half\n"
"  Alt + 1,3,7,9: quarter\n"
"  Alt + 5: center\n"
"  Alt + 0: toggle maximize"
msgstr ""
"<u>Sayı tuşları</u>\n"
"Alt + 2,4,6,8: yarım\n"
"Alt + 1,3,7,9: çeyrek\n"
"Alt + 5: ortalı\n"
"Alt + 0: ekranı kaplayacak şekilde değiştir"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"<u>Sayı tuşları</u>\n"
"Kontrol + 1-4: yarım\n"
"Kontrol + 5-8: çeyrek\n"
"Kontrol + 9: üçte bir\n"
"Kontrol +0: en büyük düğmesi"

