"<u>Keypad</u>\n"
"  Alt + 2,4,6,8: half\n"
"  Alt + 1,3,7,9: quarter\n"
"  Alt + 5: center\n"
"  Alt + 0: toggle maximize"
msgstr ""
"<u>Teclado</u>\n"
"  Alt + 2,4,6,8: mitad\n"
"  Alt + 1,3,7,9: cuarto\n"
"  Alt + 5: centro\n"
"  Alt + 0: alternar maximizar"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"<u>Teclas numéricas</u>\n"
"  Control + 1-4: mitad\n"
"  Control + 5-8: cuarto\n"
"  Control + 9: tercio\n"
"  Control + 0: alternar maximizar"

