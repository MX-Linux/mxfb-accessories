"<u>Arrow keys</u>\n"
"  Alt + Arrow: half \n"
"  Alt + LogoKey + Arrow: quarter\\n"
msgstr ""
"<u>Klawisze strzałek</u>\n"
"  Alt + Strzałka: połowa \n"
"  Alt + LogoKey + Strzałka: jedna czwarta\\n"

#: ../mxfb-tiles:24
msgid ""
"<u>Keypad</u>\n"
"  Alt + 2,4,6,8: half\n"
"  Alt + 1,3,7,9: quarter\n"
"  Alt + 5: center\n"
"  Alt + 0: toggle maximize"
msgstr ""
"<u>Klawiatura</u>\n"
"  Alt + 2,4,6,8: połowa\n"
"  Alt + 1,3,7,9: jedna czwarta\n"
"  Alt + 5: środek\n"
"  Alt + 0: maksymalizuj"

#: ../mxfb-tiles:16
msgid ""
"<u>Number keys</u> \n"
"  Control + 1-4: half\n"
"  Control + 5-8: quarter\n"
"  Control + 9: third \n"
"  Control + 0: toggle maximize"
msgstr ""
"<u>Klawisze numeryczne</u> \n"
"  Control + 1-4: połowa\n"
"  Control + 5-8: jedna czwarta\n"
"  Control + 9:  jedna trzecia\n"
"  Control + 0: maksymalizuj"

